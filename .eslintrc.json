{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "plugins": ["html"],
    "rules": {
        "no-unused-vars": ["warn", {"argsIgnorePattern": "^_"}],
        "no-empty": "warn",
        "semi": "error",
        "no-var": "error"
    }
}
